{
  "name": "javascript-development-environment",
  "version": "1.0.0",
  "description": "JavaScript development environment",
  "scripts": {
    "build": "npx babel-node buildScripts/srcServer.js",
    "prestart": "node buildScripts/startMessage.js",
    "start": "npm run build &&  open:src lint:watch",
    "open:src": "npx babel-node buildScripts/srcServer.js",
    "lint": "esw webpack.config.* src buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\""
  },
  "author": "David Feher",
  "license": "MIT",
  "dependencies": {
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "babel-loader": "8.0.6",
    "babel-preset-env": "1.7.0",
    "babel-register": "6.26.0",
    "@babel/node": "^7.6.3",
    "braces": "3.0.2",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "cheerio": "0.22.0",
    "compression": "^1.7.4",
    "cross-env": "3.1.3",
    "css-loader": "^3.2.0",
    "eslint": ">=4.18.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-watch": "^6.0.1",
    "express": "^4.17.1",
    "extract-text-webpack-plugin": "1.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "9.8.0",
    "json-schema-faker": "^0.4.7",
    "json-server": "^0.15.1",
    "localtunnel": "^2.0.0",
    "mocha": "^6.2.2",
    "nock": "^11.5.0",
    "npm-run-all": "3.1.1",
    "numeral": "1.5.3",
    "open": "^7.0.0",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "surge": "0.20.4",
    "webpack": "^4.41.2",
    "webpack-cli": "3.3.9",
    "webpack-dev-middleware": "3.7.2",
    "webpack-hot-middleware": "2.25.0",
    "webpack-md5-hash": "0.0.5"
  }
}
